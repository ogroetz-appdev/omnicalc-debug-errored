<form action="/calendar_invite/results">
  <div>
    <label for="name">Name</label>
    <input type="text" name="event_name" id="name">
  </div>
  <div>
    <label for="details">Details</label>
    <input type="text" name="event_details" id="details">
  </div>
  <div>
    <label for="event_location">Location</label>
    <input type="text" name="event_location" id="event_location">
  </div>
  <div>
    <label for="event_begins">Start time</label>
    <input type="datetime-local" name="start_date" id="event_begins">
  </div>
  <div>
    <label for="event_ends">End time</label>
    <input type="datetime-local" name="end_date" id="event_ends">
  </div>
  <button>Generate</button>
</form>
